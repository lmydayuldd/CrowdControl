package uk.ac.cam.bravo.CrowdControl.agent;

import uk.ac.cam.bravo.CrowdControl.simulator.forAgent.RoomInterface;

public interface ItineraryItemInterface {
	/**
	 * Gets the destination of this itinerary item
	 * @return Destination room
	 */
	public RoomInterface GetRoom();
	
	/**
	 * Gets the time to wait at this destination, in seconds
	 * @return Wait time, in seconds
	 */
	public int GetWaitTime();
}
